{
    "name": "surajitbasak109/laravel-ecom-express-api",
    "description": "Ecom Express API client for Laravel.",
    "keywords": [
        "surajitbasak109",
        "Laravel Ecom Express Api",
        "Laravel Ecom Express Wrapper",
        "Ecom Express SDK Module"
    ],
    "homepage": "https://github.com/surajitbasak109/surajitbasak109/laravel-ecom-express-api",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Surajit Basak",
            "email": "surajitbasak109@gmail.com",
            "homepage": "https://github.com/surajitbasak109",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^7.5|^9.0",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload-dev": {
        "psr-4": {
            "surajitbasak109\\Ecomexpress\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "surajitbasak109\\Ecomexpress\\EcomexpressServiceProvider"
            ],
            "aliases": {
                "Ecomexpress": "surajitbasak109\\Ecomexpress\\Ecomexpress"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
